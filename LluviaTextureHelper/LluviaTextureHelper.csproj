    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="ProvisionalAA.png" />
    <None Remove="ProvisionalCompanion.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="ProvisionalAA.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="ProvisionalCompanion.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
